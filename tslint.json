{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "indent": [
      true
    ],
    "interface-name": [
      false
    ],
    "arrow-parens": [
      true,
      "as-needed"
    ],
    "object-literal-sort-keys": false,
    "no-unused-expression": [true, "allow-fast-null-checks"]
  },
  "rulesDirectory": []
}
